<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef Name="Anneli_Ahmislan_8B" ParentName="Anneli_8B" Abstract="True">
		<thingCategories>
			<li>Category_Annelitrice_Ahmislan</li>
		</thingCategories>
		<recipeMaker>
			<recipeUsers>
				<li>Anneli_Ahmislan_Workbench</li>
			</recipeUsers>
